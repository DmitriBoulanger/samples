        "1": {
          "id": 1,
          "color": "#BF1B00",
          "alias": "Errors",
          "pin": true,
          "type": "lucene",
          "enable": true,
          "query": "priority: error OR fatal"
        },
        "2": {
          "id": 2,
          "color": "#00FF00",
          "alias": "Events",
          "pin": true,
          "type": "lucene",
          "enable": true,
          "query": "priority: * AND NOT (error OR fatal)"
        }
      },
      "ids": [
        0,
        1,
        2
      ]
    },
