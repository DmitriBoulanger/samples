<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config />  
	
	<!-- 
		Definition for reference Tree-Table Factory
	-->
	<bean id="factory" class="de.dbo.samples.gui.swing.treetable.api.factory.FactoryImpl" >
	    <property name="rootName" 
			value="Records (Treetable factory uses ctx)"/> 
		<property name="nodeClass" 
			value="de.dbo.samples.gui.swing.treetable.impl.ref.NodeImpl"/> 
		<property name="treetableModelClass" 
			value="de.dbo.samples.gui.swing.treetable.impl.ref.TreetableModelImpl2"/> 
		<property name="recordProvider" 
			ref="recordProvider"/> 
		<property name="treetableColumns" 
			ref="treetableColumns"/> 
	</bean>
	
	<bean id="treetableColumns" class="de.dbo.samples.gui.swing.treetable.impl.ref.TreetableColumnsImpl2">
	</bean>
	
	<bean id="recordProvider" class="de.dbo.samples.gui.swing.treetable.impl.ref.RecordProviderImpl" >
	</bean>

</beans>